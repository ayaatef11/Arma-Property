@model TenantViewModel
@{
    ViewData["Title"] = "Create";
}
<!-- Content Header (Page header) -->
<div class="content-header">

    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-success">أضافة المستأجرين </h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="/Admin/Home/Index">الصفحة الرئسية</a></li>
                    <li class="breadcrumb-item active">أرما للعقارات</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
<!-- Default box -->
<div class="card card-outline card-success">
    <div class="card-header">
        <h6 dir="rtl" class="text-success">
            <i class="fas fa-th-large"></i>
            المستأجرين
            | <a dir="rtl" href="/Admin/Categories/List" class="text-success">
                قائمة المستأجرين
            </a>

        </h6>
    </div>

    <form >
        <div class="card-body">
            <div class="form-group row">

                <div class="col-md-10">
                    <div class="form-group row">
                        <div class="col-md-2">
                            <label class="control-label">
                               الاسم
                            </label>
                        </div>
                        <div class="col-md-10">
                            <input type="text" asp-for="FullName" id="js-FullName" autocomplete="on" class="form-control" placeholder="الاسم ...........">
                            <span asp-validation-for="FullName" class="text-danger"></span>
                        </div>
                    </div>


                </div>
               
            </div>
        </div>
        <!-- /.card-body -->
        <div class="card-footer text-center">
            <button class="btn btn-success" type="submit"><i class="fas fa-plus"></i>  حفظ </button> |
            <a href="/Admin/Categories/List" class="btn btn-outline-dark"><i class="fas fa-power-off"></i> الغاء</a>
        </div>
        <!-- /.card-footer-->
    </form>
</div>
<!-- /.card -->
@section Scripts{
    <partial name="_ValidationScriptsPartial" />
}

